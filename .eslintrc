{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
  },
  "rules": {
    "array-bracket-spacing": [
      "error",
      "always"
    ],
    "arrow-parens": [
      "error",
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "computed-property-spacing": [
      "error",
      "always"
    ],
    "func-names": [
      "error",
      "never"
    ],
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-await-in-loop": 0,
    "no-loop-func": 0,
    "no-restricted-syntax": 0,
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ],
    "semi": [
      "error",
      "never"
    ],
    "space-in-parens": [
      "error",
      "always",
    ],
  }
}
